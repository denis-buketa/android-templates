<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NetworkingActivity">

    <Button
        android:id="@+id/fetchPopularMoviesRawButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fetch Popular Movies Raw"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/fetchPopularMoviesButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fetch Popular Movies"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fetchPopularMoviesRawButton" />

    <Button
        android:id="@+id/fetchPopularMoviesObservableButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fetch Popular Movies Observable"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fetchPopularMoviesButton" />

</androidx.constraintlayout.widget.ConstraintLayout>